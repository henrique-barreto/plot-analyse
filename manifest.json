{
  "name": "Page Redder",
  "version": "2.0",
  "background": {
    "scripts": [
      "startup2.js"
    ]
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "background"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Hello!",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}


