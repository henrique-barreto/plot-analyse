

{
  "name": "Page Redder",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://iqoption.com/"
  ],
  "browser_action": {
    "default_title": "Make this page red",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}


